		\subsection{Questions}

			\begin{frame}[fragile]
				\frametitle{Question 1} 

				Que fait le code suivant (switch) ?
				\begin{verbatim}
					<?php
						$a = 2;
						switch ($a) {
							default: echo 'un'; break;
							 case 1: echo 'deux'; break;
							 case 2: echo 'trois'; break;
							 case 3: echo 'quatre'; break;
						}
					?>
				\end{verbatim}
			\end{frame}

			\begin{frame}[fragile]
				\frametitle{Question 2} 

				Que donne ce code ?
				\begin{verbatim}
					<?php
	$res = array(
       array('id' => 2),
       array('id' => 3907),
       array('id' => 7814),
       array('id' => 2),
   );
$unique = array_unique($res);
var_dump($unique);
					?>
				\end{verbatim}
			\end{frame}


