		\subsection{Réponses}

			\begin{frame}
				Réponses
			\end{frame}
	
			\begin{frame}[fragile]
				\frametitle{Reponse 1} 

				Que fait le code suivant (switch) ?
				\begin{verbatim}
					<?php
						$a = 2;
						switch ($a) {
							default: echo 'un'; break;
							 case 1: echo 'deux'; break;
							 case 2: echo 'trois'; break;
							 case 3: echo 'quatre'; break;
						}
					?>
				\end{verbatim}
				Réponse : 'trois'
			\end{frame}


			\begin{frame}[fragile]
				\frametitle{Reponse 2} 

				Que donne ce code ?
				\begin{verbatim}
					<?php
	$res = array(
       array('id' => 2),
       array('id' => 3907),
       array('id' => 7814),
       array('id' => 2),
   );
$unique = array_unique($res);
var_dump($unique);
					?>
				\end{verbatim}
				Réponse : 
				\begin{verbatim}
array(0 => array('id' => 2));
				\end{verbatim}
			\end{frame}
 

			\begin{frame}[fragile]
				\frametitle{Reponse 3} 
				Quels sont les paramètres de la fonction mktime() ?
				Réponse : 
				\begin{verbatim}
int mktime ([ int $hour = date("H") [, int $minute = date("i") [, int $second = date("s") [, int $month = date("n") [, int $day = date("j") [, int $year = date("Y") [, int $is_dst = -1 ]]]]]]] )
				\end{verbatim}
			\end{frame}

			\begin{frame}[fragile]
				\frametitle{Reponse 4}
				Combien y a-t-il de fonctions commençant par array\_* ?
				Réponse : 51
			\end{frame}

